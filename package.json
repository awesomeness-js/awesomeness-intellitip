{
	"name": "awesomeness",
	"displayName": "awesomeness",
	"description": "awesomeness",
	"version": "0.0.1",
	"type": "commonjs",
	"engines": {
		"vscode": "^1.97.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript"
	],
	"main": "./extension.js",
	"contributes": { 
		"configuration": {
			"title": "Awesomeness Configuration",
			"properties": {
				"paths": {
					"type": "object",
					"description": "Mapping for path aliases",
					"default": {
						"@docs": "schemas",
						"@vertices": "api/schemas",
						"@edges": "api/edges"
					}
				}
            }
		},
		"languages": [{
			"id": "javascript"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.97.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"eslint": "^9.19.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1"
	}
}
